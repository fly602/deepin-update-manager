<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN"
        "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
    <interface name="org.deepin.UpdateManager1">
        <method name="checkUpgrade"/>
        <method name="upgrade"/>
        <property access="readwrite" type="u" name="checkUpdateMode"/>
        <property access="readwrite" type="u" name="upgradeMode"/>
        <property access="read" type="s" name="state"/>
        <property access="read" type="s" name="currentBranch"/>
        <property access="read" type="as" name="allBranches"/>
        <signal name="progress">
            <arg type="(sd)" direction="out"/>
            <annotation value="Progress"
                        name="org.qtproject.QtDBus.QtTypeName.Out0"/>
        </signal>
    </interface>
</node>